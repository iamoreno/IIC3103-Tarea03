: function() {\n      this.dataUpdate.forEach((dato) => {\n        if (!this.inserted.has(dato['ticker']+ dato['time']+'')) {\n          this.add_Data(dato);\n          this.inserted.add(dato['ticker']+ dato['time']+'');\n        }\n      });\n    },\n  },\n};\n</script>\n<style>\n.chart {\n  background-color:black;\n  padding-left: 10px;\n  margin: 20px;\n  flex: 1;\n  height: 305px;\n  width: 470px;\n  display: inline-flex;\n}\n</style>"],"sourceRoot":"src/components"}]}